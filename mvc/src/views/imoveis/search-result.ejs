  <div>
    <table>
      <thead>
        <td>Rua</td>
        <td>Cidade</td>
        <td>Estado</td>
        <td>NÃºmero</td>
        <td>Tipo</td>
      </thead>
      <tbody>
        <% imoveis.forEach(function (imovel) { %>
        <tr>
          <td><%= imovel.rua %></td>
