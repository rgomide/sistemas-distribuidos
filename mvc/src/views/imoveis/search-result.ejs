<!DOCTYPE html>
<html>

<head>
  <link href="/css/style.css" rel="stylesheet" type="text/css">
  <title>MVC</title>
</head>

<body>
  <h2>Resultados</h2>
  <% if (imoveis.length > 0) { %>
  <div>
    <table>
      <thead>
        <tr>
          <th>Rua</th>
          <th>Cidade</th>
          <th>Estado</th>
          <th>NÃºmero</th>
          <th>Tipo</th>
        </tr>
      </thead>
      <tbody>
        <% imoveis.forEach(function (imovel) { %>
        <tr>
          <td><%= imovel.rua %></td>
          <td><%= imovel.cidade %></td>
          <td><%= imovel.estado %></td>
          <td><%= imovel.numero %></td>
          <td><%= imovel.tipo %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
  <% } %>
  <a href="/">Voltar</a>
</body>

</html>
