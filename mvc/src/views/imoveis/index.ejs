<!DOCTYPE html>
<html>

<head>
  <title>MVC</title>
</head>

<body>
  <div>
    <h2>Pesquisar Imóveis</h2>
    <form method="get" action="/imoveis/search">
      <input type="text" name="tipo" placeholder="Tipo de imóvel" />
      <button type="submit">Pesquisar</button>
    </form>
  </div>
  <% if (imoveis.length> 0) { %>
  <div>
    <h2>Últimos Imóveis</h2>
    <% imoveis.forEach(function (imovel) { %>
    <div>
      <table>
        <thead>
          <td>Rua</td>
          <td>Cidade</td>
          <td>Estado</td>
          <td>Número</td>
          <td>Tipo</td>
        </thead>
        <tbody>
          <% imoveis.forEach(function (imovel) { %>
          <tr>
            <td><%= imovel.rua %></td>
            <td><%= imovel.cidade %></td>
            <td><%= imovel.estado %></td>
            <td><%= imovel.numero %></td>
            <td><%= imovel.tipo %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
    <% }) %>
  </div>
  <% } %>
</body>

</html>